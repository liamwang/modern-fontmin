{
  "name": "modern-fontmin",
  "type": "module",
  "version": "0.0.0",
  "packageManager": "pnpm@7.26.2",
  "description": "Fastest cropping font files in the browser",
  "author": "wxm",
  "license": "MIT",
  "homepage": "https://github.com/qq15725/modern-fontmin",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qq15725/modern-fontmin.git"
  },
  "bugs": {
    "url": "https://github.com/qq15725/modern-fontmin/issues"
  },
  "keywords": [
    "fontmin",
    "font-cropping",
    "typescript"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "dist/index.mjs",
  "module": "dist/index.mjs",
  "types": "types/index.d.ts",
  "browser": "dist/index.js",
  "typesVersions": {
    "*": {
      "*": [
        "./types/*",
        "./types/index.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "types"
  ],
  "scripts": {
    "dev": "vite",
    "lint": "eslint src",
    "test": "vitest --no-threads --no-isolate",
    "build": "vite build && tsc --project tsconfig.build.json",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md",
    "release": "bumpp package.json --commit \"release: v%s\" --push --all --tag"
  },
  "devDependencies": {
    "@qq15725/eslint-config": "^1.1.0",
    "@types/node": "^20.10.5",
    "bumpp": "^9.2.1",
    "conventional-changelog-cli": "^4.1.0",
    "eslint": "^8.56.0",
    "pako": "^2.1.0",
    "typescript": "^4.9.5",
    "vite": "^5.0.10",
    "vitest": "^1.1.0"
  },
  "peerDependencies": {
    "pako": "^2.*"
  }
}
